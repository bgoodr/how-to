#!/bin/bash
# Reuse the python_preamble_basic script:
#
#   For this example, we co-locate it next to the
#   non_module_python_script.py file but it could be stored into some
#   other ../ directory.
#

export PYTHON_PREAMBLE_PYTHON_MAJOR_VERSION=3

exec "$(dirname $0)/python_preamble_basic" "$0" "$@"
